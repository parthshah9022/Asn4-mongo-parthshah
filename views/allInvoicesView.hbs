
    <h1>Sales Data</h1>
    <table>
        <thead>
            <tr>
                 <th>Number</th>
                <th>Invoice ID</th>
                <th>Branch</th>
                <th>City</th>
                <th>Customer Type</th>
                <th>Product Line</th>
                <th>Name</th>
                <th>Image</th>
                <th>Unit Price</th>
                <th>Quantity</th>
                <th>Tax 5%</th>
                <th>Total</th>
                <th>Date</th>
                <th>Time</th>
                <th>Payment</th>
                <th>COGS</th>
                <th>Gross Income</th>
                <th>Rating</th>
            </tr>
        </thead>
        <tbody>
  {{#each invoice}}
    <tr>
      <td>{{incrementedIndex @index}}</td>
      <td>{{getProperty this "Invoice ID"}}</td>
      <td>{{getProperty this "Branch"}}</td>
      <td>{{getProperty this "City"}}</td>
      <td>{{getProperty this "Customer type"}}</td>
      <td>{{getProperty this "Product line"}}</td>
      <td>{{getProperty this "name"}}</td>
      <td><img src="{{getProperty this "image"}}" alt="Product Image" width="100"></td>
      <td>{{getProperty this "Unit price"}}</td>
      <td>{{getProperty this "Quantity"}}</td>
      <td>{{getProperty this "Tax 5%"}}</td>
      <td>{{getProperty this "Total"}}</td>
      <td>{{getProperty this "Date"}}</td>
      <td>{{getProperty this "Time"}}</td>
      <td>{{getProperty this "Payment"}}</td>
      <td>{{getProperty this "cogs"}}</td>
      <td>{{getProperty this "gross income"}}</td>
      <td>{{getProperty this "Rating"}}</td>
    </tr>
  {{/each}}
</tbody>

    </table>

